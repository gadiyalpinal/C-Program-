#include <stdio.h>
void main()
{
        int a, b, c, e, f;
        float g;
        printf(" Enter two no. ");
        scanf("%d %d",&a,&b);
        c = a + b;
        e = a - b;
        f = a * b;
        g = a / b;
        printf("%d + %d = %d\n",a,b,c);
        printf("%d - %d = %d\n",a,b,e);
        printf("%d * %d = %d\n",a,b,f);
        printf("%d / %d = %f\n",a,b,g);
        getch();
}